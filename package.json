{
  "name": "slogsdon.github.io",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "author": "Shane Logsdon <shane@shanelogsdon.com>",
  "license": "MIT",
  "homepage": "https://shane.logsdon.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slogsdon/slogsdon.github.io.git"
  },
  "scripts": {
    "build": "cd src && dotnet fable yarn-run js:build",
    "develop": "cd src && dotnet fable yarn-run js:develop",
    "js:build": "next build && next export -s -o public",
    "clean": "rm -rf .next public",
    "js:develop": "next",
    "format": "prettier --write 'components/**/*' 'lib/**/*' 'pages/**/*' '*.json' '*.js' .babelrc",
    "postbuild": "mv public/static/_redirects public/static/*.* public/ && sw-precache --root=public --config=sw-precache.config.js",
    "postinstall": "dotnet restore",
    "prebuild": "yarn run clean",
    "start": "serve --ssl public",
    "test": "yarn build"
  },
  "dependencies": {
    "markdown-in-js": "^1.1.3",
    "next": "^5.0.0",
    "prettier": "^1.10.2",
    "prismjs": "^1.10.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "fable-loader": "^1.1.6",
    "serve": "^6.4.9",
    "sw-precache": "^5.2.1"
  }
}
