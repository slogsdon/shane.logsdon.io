  "license": "MIT",
  "private": true,
  "x0": {
    "static": true,
    "title": "Shane Logsdon"
  },
  "scripts": {
    "build": "x0 build --static src/pages",
    "clean": "rm -rf dist",
    "format": "prettier --write 'src/**/*' '!src/data/**/*'",
