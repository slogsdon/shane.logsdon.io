    "prebuild": "$npm_execpath run clean",
    "start": "x0 src/pages"
  },
  "dependencies": {
    "@compositor/x0": "^5.0.4",
    "react": "^16.4.0",
    "react-markdown": "^3.3.2",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "contentful": "^6.1.1",
    "parcel-bundler": "^1.8.1",
    "prettier": "^1.13.4"
  }
}
